#%RAML 1.0
types:
  petshop:
    type: object
    description: Entity that represent a petShop
    properties:
      id:
        type: number
        description: identify of the petShop
        required: true
      address:
        type: string
        description: petShop address
        required: true
      contact:
        type: number
        description: petShop contact
        required: true
      schedule:
        type: string
        description: petShop schedule
        required: true
      logo:
        type: string
        description: petShop logo
        required: true
      name:
        type: string
        description: petShop name
        required: true
    
      
    